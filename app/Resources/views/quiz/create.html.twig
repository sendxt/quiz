    {{ form_row(form.title) }}
    {{ form_row(form.questions) }}

    {{ form_end(form) }}
{% endblock %}



{% block javascripts %}
    {{ parent() }}
    <script src="{{ asset('bundles/app/js/jquery.collection.js') }}"></script>
    <script src="{{ asset('bundles/app/js/quiz.js') }}"></script>
{% endblock %}